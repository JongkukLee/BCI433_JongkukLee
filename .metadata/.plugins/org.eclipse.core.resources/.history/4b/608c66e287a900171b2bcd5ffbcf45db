000128171004             DCL        &QCURSYM *CHAR 1
000129171004
000131170927             /* Pass the retrieve data in the display file then use the physical file */
000132170927             SNDRCVF
000133170927             DOWHILE    (&IN03 = '0') /* If F3, escape the loop */
000134170927                SELECT
000135171004                   WHEN       (&OPTION = '1' *OR &OPTION = '01') DO
000136171004                      RTVSYSVAL  SYSVAL(QCURSYM)  RTNVAR(&QCURSYM)
000137171004                      CHGVAR     &MSGTXT ('The currency symbol is set to ' *CAT &QIPLSTS)
000138171004                   ENDDO
000140171004                   WHEN       (&OPTION = '2' *OR &OPTION = '02') DO
000141171004                      RTVSYSVAL  SYSVAL(QIPLSTS)  RTNVAR(&QIPLSTS)
000142171004                      CHGVAR     &MSGTXT (&QIPLSTS)
000143171004                   ENDDO
000144171004                   WHEN       (&OPTION = '3' *OR &OPTION = '03') DO
000145171004                      RTVUSRPRF  MSGQ(&MSGQ) MSGQLIB(&MSGQLIB)
000146171004                      CHGVAR     &MSGTXT ('123dfdfTHE OUTPUT QUE NAME IS    ' *BCAT &MSGQLIB +
000147171004                                   *BCAT 'IN LIBRARY' *BCAT &MSGQ)
000148171004                   ENDDO
000149171004                   WHEN       (&OPTION = '4' *OR &OPTION = '04') DO
000150171004                      RTVSYSVAL  SYSVAL(QACTJOB)  RTNVAR(&QACTJOB)
000151171004                      CHGVAR     &MSGTXT (&QACTJOB)
000152171004                   ENDDO
000153171004                   WHEN       (&OPTION = '5' *OR &OPTION = '05') DO
000154171004                      RTVSYSVAL  SYSVAL(QSRLNBR)  RTNVAR(&QSRLNBR)
000155171004                      CHGVAR     &MSGTXT (&QSRLNBR)
000156171004                   ENDDO
000157171004                   WHEN       (&OPTION = '6' *OR &OPTION = '06') DO
000158171004                      call       QUSCMDLN
000159171004                      /*         RTVSYSVAL  SYSVAL(QPWDMAXLEN)  RTNVAR(&QPWDMAXLEN) */
000160171004                      /* CHGVAR     &MSGTXT (&QPWDMAXLEN)                            */
000161171004                   ENDDO
000162171004
000163171004
000164171004                   WHEN       (&OPTION = '7' *OR &OPTION = '07') DO
000165171004                      RTVUSRPRF  PRVSIGN(&PRVSIGN)
000166171004                      CHGVAR     &DATE (%SST(&PRVSIGN 4 2) *CAT '/' *CAT %SST(&PRVSIGN 6 2) +
000167171004                                   *CAT       '/' *CAT %SST(&PRVSIGN 2 2))
000180171004                      CHGVAR     &MSGTXT (&DATE)
000181171004                   ENDDO
000182171004
000183171004
000184170927                   OTHERWISE  CHGVAR &IN30 '1'
000185170927                ENDSELECT
000186170927                SNDRCVF
000187170927                CHGVAR     &IN30 '0'
000188170927                CHGVAR     &MSGTXT ' '
000189170927             ENDDO
000190170927
000191170928             SUBR       OPTION1
000192170928
000193170928                CHGVAR     &MSGTXT ('The currency symbol is set to $' )
000194170928             ENDSUBR
000195170928
000196170928             SUBR       OPTION2
000197170927                /* Retrieve system value */
000198170927                RTVSYSVAL  SYSVAL(QSECURITY) RTNVAR(&SECURITY)
000199170927                SELECT
000200170927                   WHEN       (&SECURITY = '10' ) CHGVAR  &MSGTXT  ('SECURITY LEVEL IS 10  AND +
000201170927                                NOT SUPPORTED')
000202170927                   WHEN       (&SECURITY = '20' ) CHGVAR  &MSGTXT  ('PASSWORD SECURITY ONLY AT +
000203170927                                LEVEL' *BCAT  &SECURITY)
000204170927                   WHEN       (&SECURITY = '30' ) CHGVAR    &MSGTXT ('PASSWORD AND OBJECT +
000205170927                                SECURITY AT LEVEL 30')
000206170927                   WHEN       (&SECURITY = '40' ) CHGVAR    &MSGTXT ('PASSWORD, OBJECT, AND +
000207170927                                OPERATING SYSTEM INTEGRITY AT LEVEL 40')
000208170927                   WHEN       (&SECURITY = '50' ) CHGVAR    &MSGTXT  ('PASSWORD, OBJECT, AND +
000209170927                                ENHANCED OPERATING SYSTEM INTEGRITY AT LEVEL 50' )
000210170927                ENDSELECT
000211170927             ENDSUBR
000212171004             ENDPGM
